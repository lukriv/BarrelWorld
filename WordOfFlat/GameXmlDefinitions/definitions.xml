<!-- render types -->
<robject type="renderRigidBody">
	<mesh>mesh_name</mesh>
	<material>material_name</material>
</robject>

<robject type="renderCamera">
	<name>CameraName</name>
	<projection>perspective|orthographic</projection>
	<fov>angle</fov>
	<near>near_clipping</near>
	<far>far_clipping</far>
</robject>

<!-- physics types -->
<physics type="simpleObject">
	<filename>physics_definition_filename</filename>
</physics>

<physics type="physicsRigidBody">
	<physicsShape>
		<boxShape>
			<halfSize>1.0,1.0,1.0</halfSize>
		</boxShape>
	</physicsShape>
	<mass>1.0</mass>
	<!-- <inertia>0.0,0.0,0.0</inertia> -->
</physics>

<!-- control types -->
<control type="logicManualTest"></control>
<control type="logicFreeCamera"></control>

<!-- input types -->
<input type="characterInput">
	<key action="action_name" value="key_value" />
</input>

<input type="freeCameraInput">
	<key action="action_name" value="key_value" />
</input>