<!-- render types -->
<renderRigidBody>
	<mesh>mesh_name</mesh>
	<material>material_name</material>
</renderRigidBody>

<renderCamera name="CameraName">
	<projection>perspective|orthographic</projection>
	<fov>angle</fov>
	<near>near_clipping</near>
	<far>far_clipping</far>
</renderCamera>

<!-- physics types -->
<physics type="simpleObject">
	<file>physics_definition_filename</file>
</physics>

<!-- logic types -->
<logic type="manualTest"></logic>
<logic type="freeCamera"></logic>